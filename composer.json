{
  "name": "codexten/yii-aws",
  "type": "yii2-extension",
  "description": "yii aws",
  "keywords": [
    "yiisoft",
    "aws",
    "codexten",
    "yii2"
  ],
  "require": {
    "codexten/yii-core": ">=2.0",
    "aws/aws-sdk-php": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\aws\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "aws": "config/aws.php",
      "params": "config/params.php"
    }
  }
}
